<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="GasolineApp.Views.Login">

    <StackLayout HorizontalOptions="Center" VerticalOptions="Center">

        <Entry x:Name="username" Placeholder="Username"/>
        <Entry x:Name="password" Placeholder="Password" IsPassword="True" />
        <Button x:Name="ButtonLogin" Text="Login"/>
        <Button x:Name="ButtonRegisty" Text="Registry"/>
    </StackLayout>

</ContentPage>